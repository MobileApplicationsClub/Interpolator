<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mac.interpolator.interpolator" >
    <uses-sdk android:minSdkVersion="11" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher1"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light" >
        <activity
            android:name="com.mac.interpolator.interpolator.Main_interpolator"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".About"
            android:label="About"
            android:parentActivityName="com.mac.interpolator.interpolator.Main_interpolator" />
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.mac.interpolator.interpolator.Main_interpolator" />

    </application>

</manifest>
